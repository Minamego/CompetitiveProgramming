/*
Z-algo
*/

#include <iostream>
#include<bits/stdc++.h>
using namespace std;vector<int> z_function(char* s) {
    int n = strlen(s);
    vector<int> z(n);
    for (int i = 1, l = 0, r = 0; i < n; ++i) {
        if (i <= r)
            z[i] = min (r - i + 1, z[i - l]);
        while (i + z[i] < n && s[z[i]] == s[i + z[i]])
            ++z[i];
        if (i + z[i] - 1 > r)
            l = i, r = i + z[i] - 1;
    }
    return z;
}
char str[100100];
int main()
{
    while(1)
    {
        scanf("%s" , str);
        if(str[0] == '*') break;
        vector<int>z = z_function(str);
        int n = strlen(str);
        int maxi = 0;
        for(int i = n ; i>=1 ; --i)
        {
            if(n%i==0)
            {
                int x = n/i;
                bool flag = 0;
                for(int j = x ; j<n ; j+=x) if(z[j]< x) {flag = 1 ;}
                if(flag) continue;
                maxi = i;
                break;
            }
        }
        printf("%d\n" , maxi);
    }
    return 0;
}
